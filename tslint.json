{
	"extends": "tslint:latest",
	"rules": {
		"arrow-parens": true,
		"interface-name": [true, "never-prefix"],
		"curly": true,
		"no-var-requires": false,
		"no-string-literal": false,
        "no-console": [ false ],
        "max-classes-per-file": [
            true, 50
        ]
	},
	"jsRules": {
		"curly": true
	},
	"rulesDirectory": []
}